    "directory-tree": "^3.5.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "open": "^9.1.0",
    "prompts": "^2.4.2",
    "recursive-copy": "^2.0.14",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.3",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "^0.8.15",
    "@types/through2": "^2.0.41",
    "@types/validator": "^13.11.7",
    "@vcee/sgen-types": "workspace:^",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
